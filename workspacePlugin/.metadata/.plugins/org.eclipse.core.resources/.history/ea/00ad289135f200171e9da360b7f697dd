package fr.leroideskiwis.bungeetest.commands;

import net.md_5.bungee.api.CommandSender;
import net.md_5.bungee.api.chat.TextComponent;
import net.md_5.bungee.api.connection.ProxiedPlayer;
import net.md_5.bungee.api.plugin.Command;

@SuppressWarnings("deprecation")
public class CommandeHub extends Command {

	public CommandeHub(String name) {
		super(name);
	}

	
	@Override
	public void execute(CommandSender sender, String[] args) {
		if(sender instanceof ProxiedPlayer){
			
			ProxiedPlayer p = (ProxiedPlayer)sender;
			p.sendMessage(new TextComponent("§7téléportation en cours..."));	
			
		} else {
			sender.sendMessage("§cvous devez etre joueur pour executer cette commande");
		}
		
	}

}
